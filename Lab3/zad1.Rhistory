install.packages('neuralnet')
library("neuralnet")
traininginput <-  as.data.frame(runif(50, min=1, max=10))
trainingoutput <- sqrt(traininginput )-exp(-traininginput )
trainingdata <- cbind(traininginput,trainingoutput)
colnames(trainingdata) <- c("Input","Output")
net.math <- neuralnet(Output~Input,trainingdata, hidden=10, threshold=0.01)
print(net.math)
plot(net.math)
savehistory("C:/Users/Ricoss/Desktop/R/Git/Lab3/zad1.Rhistory")
